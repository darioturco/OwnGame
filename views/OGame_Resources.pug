  title OGame
body#resources
  .contentBoxBody
    #box
      // TOP MENU
      #info.header.normal
        #clearAdvice
        #bar
          ul
            li#playerName
              | Player:
              span.textBeefy
                a.textBeefy(href='./Player.html') dturco
            li
              span (2255)
              a(href='./Highscore.html') Highscore
            li
              a(href='./Vacas.html') Vacas
            li
              a(href='./Search.html') Search
            li
              a(href='./Options.html') Options
            li
              a(href='./GodMain.html') God Mode
            li.OGameClock 07.12.2019 16:17:18
        ul#resources
          li#metal_box.metal
            .resourceIcon.metal
            span.value
              span#resources_metal 500
          li#crystal_box.crystal
            .resourceIcon.crystal
            span.value
              span#resources_crystal 500
          li#deuterium_box.deuterium
            .resourceIcon.deuterium
            span.value
              span#resources_deuterium 200
          li#energy_box.energy
            .resourceIcon.energy
            span.value
              span#resources_energy 31
          li#darkmatter_box.darkmatter.dark_highlight_tablet
            img(src='./Imagenes/Dark_matter.gif')
            span.value
              span#resources_darkmatter 0
        #message-wrapper
          // Notificaciones de mensajes y flotas
          a.comm_menu.messages(href='./Ogame_messages.html')
            span.new_msg_count.totalMessages.noMessage.news(data-new-messages='0') 0
            // Mensajes
          a.comm_menu.chat(href='./Vacas.html')
            span.new_msg_count.totalChatMessages.noMessage(data-new-messages='0') 0
            // Chat
          #messages_collapsed
            #eventboxFilled.eventToggle(style='display: none;')
              a#js_eventDetailsClosed(href='javascript:void(0);' title='More details')
              a#js_eventDetailsOpen.open(href='javascript:void(0);' title='Less detail')
            #eventboxLoading.textCenter.textBeefy(style='display: none;')
              img(height='16' width='16' src='./Imagenes/Loading.gif')
              | load...
            #eventboxBlank.textCenter(style='') No fleet movement
          #attack_alert.eventToggle.noAttack
            // <a href="https://s163-en.ogame.gameforge.com/game/index.php?page=eventList"></a>
            a(href='javascript:void(0);')
          br.clearfloat
        #helper
          a.highlight.Close(href='./OGame_Reward.html') ?
        #selectedPlanetName.textCenter Planeta Principal
      #toolbarcomponent
        // LEFTMENU
        #links
          ul#menuTable.leftmenu
            li
              span.menu_icon
                a(href='./Ogame_Reward.html' target='_self')
                  .menuImage.overview
              a.menubutton(href='./Ogame_Overview.html' target='_self')
                span.textlabel Overview
            li
              span.menu_icon
                a(href='./Ogame_ResourceSetings.html' target='_self')
                  .menuImage.resources.highlighted
              a.menubutton.selected(href='./Ogame_Resources.html' target='_self')
                span.textlabel Resources
            li
              span.menu_icon
                .menuImage.station
              a.menubutton(href='./Ogame_Facilities.html' target='_self')
                span.textlabel Facilities
            li
              span.menu_icon
                a.overlay.Right(href='./Ogame_Tecnology.html' target='_blank')
                  .menuImage.research
              a.menubutton(href='./Ogame_Research.html' target='_self')
                span.textlabel Research
            li
              span.menu_icon
                .menuImage.shipyard
              a.menubutton(href='./Ogame_Shipyard.html' target='_self')
                span.textlabel Shipyard
            li
              span.menu_icon
                .menuImage.defense
              a.menubutton(href='./Ogame_Defence.html' target='_self')
                span.textlabel Defence
            li
              span.menu_icon
                a(href='./Ogame_Movement.html' target='_self')
                  .menuImage.fleet1
              a.menubutton(href='./Ogame_Fleet.html' target='_self')
                span.textlabel Fleet
            li
              span.menu_icon
                .menuImage.galaxy
              a.menubutton(href='./Ogame_Galaxy.html' target='_self')
                span.textlabel Galaxy
          br.clearfloat
      #contentWrapper
        // CONTENT AREA
        #inhalt
          #planet(style='background-image:url(./Imagenes/Planets/Resources_Planet_1.jpg)')
            #header_text
              h2 Resources - Planeta Principal
            #slot01.slot
              a(href='./Ogame_ResourceSetings.html') Resource settings
            form(method='POST' action='' name='form' onkeyup='sendBuildRequest(null, event, false);' onsubmit='return false;')
              #detail.detail_screen
                #resources_3_large.pic
                  a.techtree_link.tooltip.overlay(href='./Ogame_Tecnology.html')
                    .techtree_img_disabled
                    span.label Techtree
                #content
                  h2 Deuterium Synthesizer
                  a#close.close_details(href='javascript:void(0);' onclick='toggleDescription(-1); return false;')
                  span.level
                    | Level 8
                    span.undermark
                  br.clearfloat
                  ul.production_info
                    li
                      | Production duration
                      span#buildDuration.time
                        | 6m 9s
                        span.undermark
                  a.demolish_link.tooltip.demolishDisabled(href='#demolish3' onclick='')
                    .demolish_img_disabled
                    span.label Tear down
                  .costs_wrap
                    p.costs_info Required to improve to level 9:
                    ul#costs
                      li.metal.tooltip
                        .resourceIcon.metal
                        .cost 5.766
                      li.crystal.tooltip
                        .resourceIcon.crystal
                        .cost 1.922
                      li.energy.tooltip
                        .resourceIcon.energy
                        .cost 82
                  .build-it_wrap
                    a.build-it(href='javascript:void(0);')
                      span Improve
                br(clear='all')
                #description
                  .txt_box
                    a.tooltip.help.overlay(href='Ogame_Tecnology.html')
                    p.description_txt(style='width: 90%')
                      | Deuterium Synthesizers draw the trace Deuterium content from the water on a planet.
          .c-left
          .c-right
          #buttonz
            .header
              h2 Resource buildings
            .content
              ul#building
                li#button1.on
                  // disable = rayado; on = prendido; off = blanco y negro
                  .supply1
                    .buildingimg
                      a#details.detail_button.tooltip.slideIn(href='#' onclick='toggleDescription(1);return false;')
                        span.ecke
                          span.level
                            span.textlabel Metal Mine
                            | 0
                li#button2.on
                  .supply2
                    .buildingimg
                      a#details.detail_button.tooltip.slideIn(href='#' onclick='toggleDescription(2);return false;')
                        span.ecke
                          span.level
                            span.textlabel Crystal Mine
                            | 0
                li#button3.on
                  .supply3
                    .buildingimg
                      a#details.detail_button.tooltip.slideIn(href='#' onclick='toggleDescription(3);return false;')
                        span.ecke
                          span.level
                            span.textlabel Deuterium Synthesizer
                            | 0
                li#button4.on
                  .supply4
                    .buildingimg
                      a#details.detail_button.tooltip.slideIn(href='#' onclick='toggleDescription(4);return false;')
                        span.ecke
                          span.level
                            span.textlabel Solar Plant
                            | 0
                li#button5.on
                  .supply12
                    .buildingimg
                      a#details.detail_button.tooltip.slideIn(href='#' onclick='toggleDescription(5);return false;')
                        span.ecke
                          span.level
                            span.textlabel Fusion Reactor
                            | 0
                li#button6.on
                  .supply212
                    .buildingimg
                      a#details.detail_button.tooltip.slideIn(href='#' onclick='toggleDescription(44);return false;')
                        span.ecke
                          span.level
                            span.textlabel Solar Satellite
                            | 0
              ul#storage
                li#button7.on
                  .supply22
                    .buildingimg
                      a#details.tooltip.slideIn(href='#' onclick='toggleDescription(6);return false;')
                        span.ecke
                          span.level
                            span.textlabel Metal Storage
                            | 0
                li#button8.on
                  .supply23
                    .buildingimg
                      a#details.tooltip.slideIn(href='#' onclick='toggleDescription(7);return false;')
                        span.ecke
                          span.level
                            span.textlabel Crystal Storage
                            | 0
                li#button9.on
                  .supply24
                    .buildingimg
                      a#details.tooltip.slideIn(href='#' onclick='toggleDescription(8);return false;')
                        span.ecke
                          span.level
                            span.textlabel Deuterium Tank
                            | 0
              .footer
              br.clearfloat
      #rechts
        // RIGHTMENU
        #norm
          #myWorlds
            #countColonies
              p.textCenter
                span 1/1
                |  Planets
            #planetList
              #planet-33655142.smallplanet
                a.planetlink.Right.Close(href='https://s163-en.ogame.gameforge.com/game/index.php?page=overview&cp=33655142')
                  img.planetPic.js_replace2x(alt='Planet' src='./Imagenes/Planets/Planet_1.png' width='48' height='48')
                  span.planet-name Planeta Principal
                  span.planet-koords [2:289:4]
