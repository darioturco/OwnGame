extends Ogame_Layout
block content
  #inhalt
    #planet.shortHeader
      h2 Fleet Dispatch - #{basic.planets[basic.numPlanet].name}
    .fleetStatus
      #slots.fleft
        .fleft
          span.tooltip.advice
            span Fleets :
            |  #{info.slot}/#{info.maxSlot}
        .fleft
          span.tooltip.advice
            span Expeditions:
            |  #{info.expeditions}/#{info.maxExpeditions}
        br.clearfloat
      #movements.fright
        a.tooltip.dark_highlight_tablet(href='Ogame_Movement.html')
          img(src='./Imagenes/MiniFleet.gif')
          span Fleet movement
      br.clearfloat
    .c-left
    .c-right
    #buttonz
      .header
        h2
          span.h_battleships Combat ships
          span.h_civilships Civil ships
      #selectNaves.content
        #battleships
          ul#military
            li#button204(class=((info.fleets.lightFighter > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(0);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.lightFighter}
              input#ship_204.fleetValues(name='' tabindex='1' type='text' pattern='[0-9,.]*' value='' )
            li#button205(class=((info.fleets.heavyFighter > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(1);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.heavyFighter}
              input#ship_205.fleetValues(name='' tabindex='2' type='text' pattern='[0-9,.]*' value='')
            li#button206(class=((info.fleets.cruiser > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(2);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.cruiser}
              input#ship_206.fleetValues(name='' tabindex='3' type='text' pattern='[0-9,.]*' value='')
            li#button207(class=((info.fleets.battleship > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(3);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.battleship}
              input#ship_207.fleetValues(name='' tabindex='4' type='text' pattern='[0-9,.]*' onchange="checkIntInput(this, 0, 31); checkShips('shipsChosen'); return false;" onkeyup="checkIntInput(this, 0, 31); checkShips('shipsChosen'); return false;" onkeypress='return submitOnEnter(event);' onfocus='clearInput(this); return false;' value='')
            li#button215(class=((info.fleets.battlecruiser > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(4);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.battlecruiser}
              input#ship_215.fleetValues(name='' tabindex='5' type='text' pattern='[0-9,.]*' value='')
            li#button211(class=((info.fleets.bomber > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(5);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.bomber}
              input#ship_211.fleetValues(name='' tabindex='6' type='text' pattern='[0-9,.]*' value='')
            li#button213(class=((info.fleets.destroyer > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(6);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.destroyer}
              input#ship_213.fleetValues(name='' tabindex='7' type='text' pattern='[0-9,.]*' value='')
            li#button214(class=((info.fleets.deathstar > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(7);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.deathstar}
              input#ship_214.fleetValues(name='' tabindex='8' type='text' pattern='[0-9,.]*' value='')
        #spacer
        #civilships
          ul#civil
            li#button202(class=((info.fleets.smallCargo > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(8);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.smallCargo}
              input#ship_202.fleetValues(name='' tabindex='9' type='text' pattern='[0-9,.]*' value='')
            li#button203(class=((info.fleets.largeCargo > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(9);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.largeCargo}
              input#ship_203.fleetValues(name='' tabindex='10' type='text' pattern='[0-9,.]*' onchange="checkIntInput(this, 0, 10); checkShips('shipsChosen'); return false;" onkeyup="checkIntInput(this, 0, 10); checkShips('shipsChosen'); return false;" onkeypress='return submitOnEnter(event);' onfocus='clearInput(this); return false;' value='')
            li#button208(class=((info.fleets.colony > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(10);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.colony}
              input#ship_208.fleetValues(name='' tabindex='11' type='text' pattern='[0-9,.]*' value='')
            li#button209(class=((info.fleets.recycler > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(11);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.recycler}
              input#ship_209.fleetValues(name='' tabindex='12' type='text' pattern='[0-9,.]*' value='')
            li#button210(class=((info.fleets.espionageProbe > 0) ? 'on' : 'off'))
              .buildingimg
                a.tooltip(href='javascript:void(0);' onClick='selectAllNave(12);')
                  span.ecke
                    span.level
                      span.textlabel
                      | #{info.fleets.espionageProbe}
              input#ship_210.fleetValues(name='' tabindex='13' type='text' pattern='[0-9,.]*' value='')
        .clearfloat
        .footer
    #buttonz
      .header
      .content
        table#mission(cellpadding='0' cellspacing='0')
          tbody
            tr
              th Take off location:
              th
              th Destination:
              th
            tr
              td#start.border5px
                .planetname #{basic.planets[basic.numPlanet].name}
                .target
                  a.planet_source_selected
                    span.textlabel Planet
                  a.moon_source
                    span.textlabel Moon
                  br.clearfloat
                .coords
                  | Coordinates:
                  span(style='color:#ffffff; font-weight:bold') #{basic.planets[basic.numPlanet].coordinates.galaxy}:#{basic.planets[basic.numPlanet].coordinates.system}:#{basic.planets[basic.numPlanet].coordinates.pos}
              td#distance
                #distanceValue 5
                .coords Distance
              td#target.border5px
                #targetPlanetName.planetname Planet
                .target
                  a#pbutton.planet_selected(href='javascript:void(0);' onclick='pressPlanetMoonDebris(1); return false;')
                  a#mbutton.moon(href='javascript:void(0);' onclick='pressPlanetMoonDebris(2); return false;')
                  a#dbutton.debris(href='javascript:void(0);' onclick='pressPlanetMoonDebris(3); return false;')
                  br.clearfloat
                .coords
                  | Coordinates:
                  br
                  input#galaxy.galaxy.hideNumberSpin(name='galaxy' type='text' pattern='[0-9]*' size='1' value=objCord.galaxy)
                  input#system.system.hideNumberSpin(name='system' type='text' pattern='[0-9]*' size='3' value=objCord.system)
                  input#position.planet.hideNumberSpin(name='position' type='text' pattern='[0-9]*' size='2' value=objCord.pos)
              td#shortcuts
                div
                  span(id='shortlinks tips') Shortcuts:
                  .glow
                    select#slbox.planets.dropdownInitialized(size='1' onchange='shortLinkChange();updateVariables();' style='display: none;')
                      option(value='-') -
                      option(value='2#289#4#1#Planeta Principal' data-html-prepend='<figure class="planetIcon planet tooltip js_hideTipOnMobile" title="Planet"></figure>')
                        | Planeta Principal [2:289:4]
                      option(value='2#289#4#3#Moon' data-html-prepend='<figure class="planetIcon moon tooltip js_hideTipOnMobile" title="Moon"></figure>')
                        | Moon [2:289:4]
                      option(value='2#290#5#1#Volcancito' data-html-prepend='<figure class="planetIcon planet tooltip js_hideTipOnMobile" title="Planet"></figure>')
                        | Volcancito [2:290:5]
                      option(value='1#413#5#1#need moon' data-html-prepend='<figure class="planetIcon planet tooltip js_hideTipOnMobile" title="Planet"></figure>')
                        | need moon [1:413:5]
                      option(value='2#286#3#1#Desierto' data-html-prepend='<figure class="planetIcon planet tooltip js_hideTipOnMobile" title="Planet"></figure>')
                        | Desierto [2:286:3]
                    span.dropdown.currentlySelected.planets(rel='dropdown662' style='width: 144px;')
                      a.undefined(data-value='-' rel='dropdown662' href='javascript:void(0);') -
                div(style='padding-top:12px;')
                  span(id='combatunits tips') Combat forces:
                  .glow
                    select#aksbox.combatunits.dropdownInitialized(size='1' name='acsValues' onchange='shortLinkChange(true);updateVariables();handleUnion()' style='display: none;')
                      option(value='-') -
                    span.dropdown.currentlySelected.combatunits(rel='dropdown6' style='width: 144px;')
                      a.undefined(data-value='-' rel='dropdown6' href='javascript:void(0);') -
        .briefing.border5px
          h2 Briefing
          ul#fleetBriefingPart1.fleetBriefing
            li
              | Duration of flight (one way):
              span#duration.value 0:01:23 h
            li
              | Deuterium consumption:
              span.value
                span#consumption
                  span.undermark 1 (1%)
            li
              | Speed: (max.
              span#maxspeed 25.000
              | ):
              #speedLinks
                a.dark_highlight_tablet.speed.low(href='javascript:void(0);' data-value='1') 10
                a.dark_highlight_tablet.speed.low(href='javascript:void(0);' data-value='2') 20
                a.dark_highlight_tablet.speed.low(href='javascript:void(0);' data-value='3') 30
                a.dark_highlight_tablet.speed.middle(href='javascript:void(0);' data-value='4') 40
                a.dark_highlight_tablet.speed.middle(href='javascript:void(0);' data-value='5') 50
                a.dark_highlight_tablet.speed.middle(href='javascript:void(0);' data-value='6') 60
                a.dark_highlight_tablet.speed.high(href='javascript:void(0);' data-value='7') 70
                a.dark_highlight_tablet.speed.high(href='javascript:void(0);' data-value='8') 80
                a.dark_highlight_tablet.speed.high(href='javascript:void(0);' data-value='9') 90
                a.dark_highlight_tablet.speed.high.selected(href='javascript:void(0);' data-value='10') 100
                |                        %
          ul#fleetBriefingPart1_2.fleetBriefing
            li
              | Arrival:
              span.value
                span#arrivalTime 24.11.19 3:48:35
                |  Clock
            li
              | Return:
              span.value
                span#returnTime 24.11.19 3:49:58
                |  Clock
            li
              | Empty cargobays:
              span#storage.value
                span.undermark 0
        .footer
    #buttonz
      .header
        h2 Select mission for target:
      .content
        ul#missions
          li#button15.off
            a#missionButton15(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Expedition
          li#button7.off
            a#missionButton7(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Colonisation
          li#button8.off
            a#missionButton8(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Recycle debris field
          li#button3.on
            a#missionButton3.selected(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Transport
          li#button4.on
            a#missionButton4(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Deployment
          li#button6.off
            a#missionButton6(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Espionage
          li#button5.off
            a#missionButton5(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel ACS Defend
          li#button1.off
            a#missionButton1(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Attack
          li#button2.off
            a#missionButton2(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel ACS Attack
          li#button9.off
            a#missionButton9(href='javascript:void(0);' onclick='pressButtonMision();')
              span.textlabel Moon Destruction
        #missionNameWrapper
          | Mission:
          span#missionName.missionName Transport
          p.mission_description Transports your resources to other planets.
        .footer
    #sendfleet
      .header
        h2
          span.h_half_w Send Fleet
          span.h_half_w Load resources
      .content
        form(name='sendForm' method='post' action='')
          #wrap
            #resources.border5px
              .res_wrap.border3px
                .resourceIcon.metal.tooltip
                .res
                  input#metal.checkThousandSeparator.hideNumberSpin(type='text' pattern='[0-9,.]*' name='metal' tabindex='1' value='0' onblur='updateVariables();' onkeyup="checkRessourceByType('metal'); updateVariables();" onkeypress='return submitOnEnter(event);')
                  a.min(href='javascript:void(0);' onclick="resetByRessourceType('metal'); return false;")
                    img(src='../Imagenes/LeftMiniArrow.gif')
                  a.max(href='javascript:void(0);' onclick='maxMetal();updateVariables(); return false;')
                    img(src='../Imagenes/RightMiniArrow.gif')
              .res_wrap.border3px
                .resourceIcon.crystal.tooltip
                .res
                  input#crystal.checkThousandSeparator.hideNumberSpin(type='text' pattern='[0-9,.]*' name='crystal' value='0' tabindex='2' onblur='updateVariables();' onkeyup="checkRessourceByType('crystal'); updateVariables();" onkeypress='return submitOnEnter(event);')
                  a.min(href='javascript:void(0);' onclick="resetByRessourceType('crystal'); return false;")
                    img(src='../Imagenes/LeftMiniArrow.gif')
                  a.max(href='javascript:void(0);' onclick='maxCrystal();updateVariables(); return false;')
                    img(src='../Imagenes/RightMiniArrow.gif')
              .res_wrap.border3px
                .resourceIcon.deuterium.tooltip
                .res
                  input#deuterium.checkThousandSeparator.hideNumberSpin(type='text' pattern='[0-9,.]*' name='deuterium' value='0' tabindex='3' onblur='updateVariables();' onkeyup="checkRessourceByType('deuterium'); updateVariables();" onkeypress='return submitOnEnter(event);')
                  a.min(href='javascript:void(0);' onclick="resetByRessourceType('deuterium');return false;")
                    img(src='../Imagenes/LeftMiniArrow.gif')
                  a.max(href='javascript:void(0);' onclick='maxDeuterium();updateVariables();return false;')
                    img(src='../Imagenes/RightMiniArrow.gif')
              #loadAllResources
                a#allresources(href='javascript:void(0);' onclick='loadAllResources()')
                |                             all resources
              #loadRoom
                | cargo bay:
                .fleft.bar_container(data-current-amount='0' data-capacity='31250')
                  .filllevel_bar.filllevel_undermark(style='width: 0%;')
                .tooltip(title='Available space / Max. cargo space')
                  span#remainingresources
                    span.undermark 31.250
                  |                                 /
                  span#maxresources 31.250

            #roundup.border5px
              a#start.on(href='' onclick='trySubmit();return false;')
                span(style='padding-top:9px;') Send fleet
              br.clearfloat
          br.clearfloat
        .footer
