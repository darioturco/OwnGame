extends Ogame_Layout
block content
  #inhalt
    #planet(style='background-image:url(./Imagenes/Planets/Overview_Planet_1.jpg);')
      #detailWrapper
        #header_text
          h2
            a.openPlanetRenameGiveupBox(href='javascript:void(0);')
              p.planetNameOverview Overview -
              span#planetNameHeader Planeta Principal
              img.hinted(src='./Imagenes/Overview/Gear.gif' width='16' height='16')
        #detail.detail_screen
          #techDetailLoading
        #planetdata
          .overlay
          #planetDetails
            //
              <table cellpadding="0" cellspacing="0" width="100%">
              <tbody><tr>
              <td class="desc">
              <span id="diameterField">Diameter:</span>
              </td>
              <td class="data">
              <span id="diameterContentField">12.800km (<span>23</span>/<span>188</span>)</span>
              </td>
              </tr>
              <tr>
              <td class="desc">
              <span id="temperatureField">Temperature:</span>
              </td>
              <td class="data">
              <span id="temperatureContentField">35°C to 75°C</span>
              </td>
              </tr>
              <tr>
              <td class="desc">
              <span id="positionField">Position:</span>
              </td>
              <td class="data">
              <span id="positionContentField"><a href="https://s163-en.ogame.gameforge.com/game/index.php?page=galaxy&amp;galaxy=2&amp;system=289&amp;position=4">[2:289:4]</a></span>
              </td>
              </tr>
              <tr>
              <td class="desc">
              <span id="scoreField">Points:</span></td>
              <td class="data">
              <span id="scoreContentField"><a href="https://s163-en.ogame.gameforge.com/game/index.php?page=highscore">10 (Place 2.255 of 3.711)</a></span>
              </td>
              </tr>
              <tr>
              <td class="desc">
              <span id="honorField">Honour points:</span></td>
              <td class="data ">
              <span id="honorContentField">0</span>
              </td>
              </tr>
              </tbody></table>
          #planetOptions
            a.dark_highlight_tablet.float_right.openPlanetRenameGiveupBox(href='javascript:void(0);')
              span.planetMoveOverviewGivUpLink Abandon/Rename
              span.planetMoveIcons.settings.planetMoveGiveUp.icon
      #buffBar.sliderWrapper
        .add_item(data-uuid='' data-id='')
          a.activate_item.border3px(href='javascript:void(0);' ref='1')
        .anythingSlider.anythingSlider-default.activeSlider(style='width: 532px; height: 36px;')
          .anythingWindow
            ul.active_items.anythingBase.horizontal(style='width: 532px; left: 0px;')
              li.panel.activePage(style='width: 532px; height: 36px;')
          span.arrow.back.disabled(style='display: none;')
            a(href='https://s163-en.ogame.gameforge.com/game/index.php?page=overview#')
              span &laquo;
          span.arrow.forward.disabled(style='display: none;')
            a(href='https://s163-en.ogame.gameforge.com/game/index.php?page=overview#')
              span &raquo;
    .c-left
    .c-right
    .clearfloat
    .clearfloat
